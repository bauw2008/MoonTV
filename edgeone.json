{
  "name": "vidora",
  "framework": "nextjs",
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "nodeVersion": "22.11.0",
  
  "env": {
    "EDGEONE_PAGES": "1"
  },
  "headers": [{
    "source": "/fonts/inter-var-latin.woff2",
    "headers": [{
      "key": "Cache-Control",
      "value": "public, max-age=31536000, immutable"
    }]
  }]
}