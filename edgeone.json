{
  "name": "vidora",
  "framework": "nextjs",
  "buildCommand": "pnpm run build",
  "outputDirectory": ".next/standalone", 
  "nodeVersion": "20.18.0",
  "functions": {
	  "runtime": "nodejs",
	  "entry": "server.js"
  },
  
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 1 * * *"
    }
  ]
}
