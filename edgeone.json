{
  "name": "vidora",
  "framework": "nextjs",
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "serverConfig": {
    "runtime": "nodejs20"
  },
  "env": [
    {
      "key": "EDGEONE_PAGES",
      "value": "1",
      "type": "all"
    }
  ],
  "headers": [
    {
      "source": "/fonts/inter-var-latin.woff2",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
